<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.king.library.book.mapper.BookCatalogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.king.library.book.model.BookCatalog">
        <id column="catalog_id" property="catalogId" />
        <result column="catalog_name" property="catalogName" />
        <result column="areas" property="areas" />
        <result column="floors" property="floors" />
        <result column="create_date" property="createDate" />
        <result column="create_user_id" property="createUserId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        catalog_id, catalog_name, areas, floors, create_date, create_user_id
    </sql>

</mapper>
